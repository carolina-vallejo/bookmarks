{
  "name": "Carolina's most awesome Bookmarks",
  "version": "1.0",
  "description": "Booooookmarks!!!",
  "permissions": [
    "bookmarks",
    "activeTab",
    "declarativeContent",
    "storage",
    "chrome://favicon/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "assets/images/get_started16.png",
      "32": "assets/images/get_started32.png",
      "48": "assets/images/get_started48.png",
      "128": "assets/images/get_started128.png"
    }
  },
  "options_page": "index.html",
  "icons": {
    "16": "assets/images/get_started16.png",
    "32": "assets/images/get_started32.png",
    "48": "assets/images/get_started48.png",
    "128": "assets/images/get_started128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-Db8eNpj9Svry8gof9jGiBp2XdUNucUxTSU1ppBDiO6E='; object-src 'self'; img-src chrome://favicon;"
}
