{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["bookmarks", "activeTab", "declarativeContent", "storage", "chrome://favicon/"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/images/get_started16.png",
      "32": "assets/images/get_started32.png",
      "48": "assets/images/get_started48.png",
      "128": "assets/images/get_started128.png"
    }
  },
  "options_page": "index.html",
  "icons": {
    "16": "assets/images/get_started16.png",
    "32": "assets/images/get_started32.png",
    "48": "assets/images/get_started48.png",
    "128": "assets/images/get_started128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-n6hP7CWAomq3EYZ3b6LSDb2GgFRx9RiKSrvje+iglpE=' 'sha256-89K7jBBFXCX5X0DLYvZEym6gWmXcoT86hDxXgo8oogE='; object-src 'self'; img-src chrome://favicon;"
}
